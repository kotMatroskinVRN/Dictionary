            xmlns:fx="http://javafx.com/fxml/1"
            stylesheets="style/ChooseDictionary.css"
            fx:controller="home.fifteen.dictionary.controller.ChooserController">
   <center>
       <ScrollPane BorderPane.alignment="CENTER">
           <VBox fx:id="sourceListView"/>
       </ScrollPane>
   </center>
   <top>
       <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0"
                 BorderPane.alignment="CENTER">
           <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
           </padding>
           <!--            <Button mnemonicParsing="false" text="All" />-->
           <!--            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="72.0" text="Internet" />-->
           <!--            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="73.0" text="Local" />-->
           <!--            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="73.0" text="Encrypted" />-->
       </FlowPane>
   </top>
   <bottom >
       <FlowPane id="back">
            <Button  mnemonicParsing="false" onAction="#switchScene" prefWidth="72.0" text="Back" />
       </FlowPane>
   </bottom>
</BorderPane>
